{
  "name": "gbraver-burst-core",
  "description": "braver burst core",
  "version": "1.18.0",
  "author": "y.takeuchi",
  "bugs": {
    "url": "https://github.com/kaidouji85/gbraver-burst-core/issues",
    "email": "kaidouji85@gmail.com"
  },
  "dependencies": {
    "ramda": "^0.29.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/ramda": "^0.29.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "codehawk-cli": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "madge": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": "^18.15.0"
  },
  "files": [
    "lib/"
  ],
  "homepage": "https://github.com/kaidouji85/gbraver-burst-core#readme",
  "keywords": [
    "gbraver-burst"
  ],
  "license": "ISC",
  "main": "lib/src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaidouji85/gbraver-burst-core"
  },
  "scripts": {
    "build": "run-s clean transpile",
    "clean": "rimraf lib",
    "code-analysis": "codehawk src",
    "code-format": "run-s code-format:src code-format:test",
    "code-format:src": "prettier --write src",
    "code-format:test": "prettier --write test",
    "detect-circular": "run-s detect-circular:src detect-circular:test",
    "detect-circular:src": "madge --extensions ts -c src",
    "detect-circular:test": "madge --extensions ts -c test",
    "lint": "run-s lint:src lint:test",
    "lint-fix": "run-s lint-fix:src lint-fix:test",
    "lint-fix:src": "eslint --fix src",
    "lint-fix:test": "eslint --fix test",
    "lint:src": "eslint src",
    "lint:test": "eslint test",
    "prepublishOnly": "run-s clean lint test build",
    "test": "jest",
    "transpile": "tsc -d --outDir lib",
    "type-check": "tsc --noEmit",
    "update-snapshot": "cross-env SHOULD_UPDATE_SNAPSHOT=true jest"
  }
}
