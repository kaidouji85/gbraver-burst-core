{
  "name": "gbraver-burst-core",
  "version": "1.15.2",
  "description": "braver burst core",
  "engines": {
    "node": "^16.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaidouji85/gbraver-burst-core"
  },
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "run-s clean type-check lint lint:test type-check:test test transpile copy-type-definition",
    "transpile": "cross-env BABEL_ENV=production babel src/ -d lib/",
    "type-check": "flow check src",
    "type-check:test": "flow check test",
    "copy-type-definition": "flow-copy-source src lib",
    "clean": "rimraf lib",
    "test": "jest",
    "update-snapshot": "cross-env SHOULD_UPDATE_SNAPSHOT=true jest",
    "lint": "eslint src",
    "lint:test": "eslint test",
    "lint:all": "run-s lint lint:test",
    "lint-fix": "eslint --fix src",
    "lint-fix:test": "eslint --fix test",
    "lint-fix:all": "run-s lint-fix lint-fix:test",
    "code-format": "prettier --write src",
    "code-format:test": "prettier --write test",
    "code-format:all": "run-s code-format code-format:test",
    "code-analysis": "codehawk src"
  },
  "files": [
    "lib/"
  ],
  "author": "y.takeuchi",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-flow": "^7.18.6",
    "@babel/register": "^7.18.9",
    "codehawk-cli": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "flow-bin": "^0.194.0",
    "flow-copy-source": "^2.0.9",
    "flow-typed": "^3.8.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "ramda": "^0.28.0"
  }
}
