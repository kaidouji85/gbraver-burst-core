{
  "name": "gbraver-burst-core",
  "version": "0.2.13",
  "description": "braver burst core",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaidouji85/gbraver-burst-core"
  },
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm-run-all clean flow flow:test test transpile copy-flow",
    "transpile": "cross-env BABEL_ENV=production babel src/ -d lib/",
    "flow": "flow check src",
    "flow:test": "flow check test",
    "copy-flow": "flow-copy-source src lib",
    "clean": "rimraf lib",
    "clean:coverage": "rimraf .nyc_output coverage",
    "test": "ava test/src/",
    "ava": "ava",
    "coverage": "nyc ava test/src/",
    "coverage:html": "nyc report --reporter=html ava test/src/"
  },
  "files": [
    "lib/"
  ],
  "author": "y.takeuchi",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-flow-strip-types": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.0",
    "ava": "^1.4.1",
    "cross-env": "^5.2.0",
    "flow-bin": "^0.97.0",
    "flow-copy-source": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "require": [
      "@babel/register"
    ]
  }
}
