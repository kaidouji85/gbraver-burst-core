  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/ramda": "^0.28.23",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "codehawk-cli": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.4",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "^18.14.1"
  },
  "files": [
    "lib/"
  ],
  "homepage": "https://github.com/kaidouji85/gbraver-burst-core#readme",
