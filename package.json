{
  "name": "gbraver-burst-core",
  "version": "0.2.2",
  "description": "braver burst core",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaidouji85/gbraver-burst-core"
  },
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "npm-run-all flow:test ava:all",
    "build": "npm-run-all flow clean babel copy-flow",
    "babel": "babel src/ -d lib/",
    "flow": "flow check src",
    "flow:test": "flow check test",
    "copy-flow": "flow-copy-source src lib",
    "clean": "rimraf lib",
    "ava": "ava",
    "ava:all": "ava test/"
  },
  "files": [
    "lib/"
  ],
  "author": "y.takeuchi",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "flow-bin": "^0.72.0",
    "flow-copy-source": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "ramda": "^0.25.0"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  }
}
