{
  "name": "gbraver-burst-core",
  "version": "0.1.7",
  "description": "braver burst core",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaidouji85/gbraver-burst-core"
  },
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "npm-run-all flow:test clean:test babel:test ava",
    "build": "npm-run-all flow clean babel copy-flow",
    "babel": "babel src/ -d lib/",
    "babel:test": "babel test/ -d test-build/test && babel src/ -d test-build/src",
    "flow": "flow src",
    "flow:test": "flow test",
    "copy-flow": "flow-copy-source src lib",
    "clean": "rimraf lib",
    "clean:test": "rimraf test-build",
    "ava": "ava **/test-build/**/*-test.js"
  },
  "files": [
    "lib/"
  ],
  "author": "y.takeuchi",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "flow-bin": "^0.69.0",
    "flow-copy-source": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2"
  }
}
