{
  "name": "gbraver-burst-core",
  "version": "0.2.6",
  "description": "braver burst core",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaidouji85/gbraver-burst-core"
  },
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm-run-all clean:build type-check:src type-check:test test transpile copy-flow",
    "transpile": "cross-env BABEL_ENV=production babel src/ -d lib/",
    "type-check:src": "flow check src",
    "type-check:test": "flow check test",
    "copy-flow": "flow-copy-source src lib",
    "clean:build": "rimraf lib",
    "clean:coverage": "rimraf .nyc_output coverage",
    "test": "ava test/src/",
    "test:target": "ava",
    "coverage": "nyc ava test/src/",
    "coverage:html": "nyc report --reporter=html ava test/src/"
  },
  "files": [
    "lib/"
  ],
  "author": "y.takeuchi",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "flow-bin": "^0.79.0",
    "flow-copy-source": "^2.0.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "ramda": "^0.25.0"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  }
}
