{
  "name": "gbraver-burst-core",
  "version": "1.16.1-beta",
  "description": "braver burst core",
  "engines": {
    "node": "^16.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaidouji85/gbraver-burst-core"
  },
  "main": "lib/src/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "run-s clean type-check lint lint:test test transpile",
    "transpile": "tsc -d --outDir lib",
    "type-check": "tsc --noEmit",
    "clean": "rimraf lib",
    "test": "jest",
    "update-snapshot": "cross-env SHOULD_UPDATE_SNAPSHOT=true jest",
    "lint": "eslint src",
    "lint:test": "eslint test",
    "lint:all": "run-s lint lint:test",
    "lint-fix": "eslint --fix src",
    "lint-fix:test": "eslint --fix test",
    "lint-fix:all": "run-s lint-fix lint-fix:test",
    "code-format": "prettier --write src",
    "code-format:test": "prettier --write test",
    "code-format:all": "run-s code-format code-format:test",
    "code-analysis": "codehawk src"
  },
  "files": [
    "lib/"
  ],
  "author": "y.takeuchi",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/ramda": "^0.28.20",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "codehawk-cli": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "ramda": "^0.28.0"
  }
}
