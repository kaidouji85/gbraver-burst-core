{
  "name": "gbraver-burst-core",
  "version": "0.5.2",
  "description": "braver burst core",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaidouji85/gbraver-burst-core"
  },
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm-run-all clean type-check type-check:test test transpile copy-type-definition",
    "transpile": "cross-env BABEL_ENV=production babel src/ -d lib/",
    "type-check": "flow check src",
    "type-check:test": "flow check test",
    "copy-type-definition": "flow-copy-source src lib",
    "clean": "rimraf lib",
    "clean:coverage": "rimraf .nyc_output coverage",
    "test": "ava",
    "lint": "eslint src",
    "lint:test": "eslint test",
    "coverage": "nyc ava",
    "coverage:html": "nyc --reporter=lcov --reporter=text-summary ava"
  },
  "files": [
    "lib/"
  ],
  "author": "y.takeuchi",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.5.5",
    "ava": "^2.3.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "flow-bin": "^0.107.0",
    "flow-copy-source": "^2.0.8",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "require": [
      "@babel/register"
    ],
    "files": [
      "test/src/**/*-test.js"
    ]
  }
}
